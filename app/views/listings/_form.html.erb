<h1>Listings Form</h1>

<%= render "layouts/errors", :object => @listing %>

<%= form_for [@user, @listing] do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :listingtype %>
  <%= f.text_field :listingtype %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.submit %>
<% end %>
